{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./Content/out-tsc",
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2017", "dom" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
